  <body>
    <h1>Edit Venue</h1>
    <h2>Here are the Venues you have added, Please edit the field and press update at the end of the table:</h2>
    <style>
      table, th, td {
        border:1px solid black;
      }
      input{
        width: 95%;
      }
    </style>
    <table style="width:100%">
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Address</th>
        <th>Guests</th>
        <th>Stage</th>
        <th>Food</th>
        <th>Price</th>
        <th>ExtraInfo</th>
      </tr>
      <% allfoods.forEach(function(food) { %>
        <tr>
          <form method="post" action="/uVenue">
            <th><input id="pid" type="text" name="pid" value="<%= food.ID %>" ></th>
            <th><input id="name" type="text" name="name" value="<%= food.Name %>"></th>
            <th><input id="addr" type="text" name="addr" value="<%= food.Address %>"></th>
            <th><input id="g" type="text" name="g" value="<%= food.Guests %>"></th>
            <th><input id="stage" type="text" name="stage" value="<%= food.Stage %>"></th>
            <th><input id="food" type="text" name="food" value="<%= food.Food %>"></th>
            <th><input id="price" type="text" name="price" value="<%= food.Price %>"></th>
            <th><input id="eInfo" type="text" name="eInfo" value="<%= food.ExtraInfo %>"></th>
            <th><button type="submit" id="btn" value="Update">Update</button></th>
          </form> 
          <form method="post" action="/dVenue">
            <th><input id="fid" type="text" name="pid" readonly="readonly" value="<%= food.ID %>" ></th>
            <th><button type="submit" id="btnd" onclick="myFunction()" value="Delete">Delete</button></th>
            <script>
              function myFunction(){
                alert("Are you sure you want to delete this item?");
              }
            </script>
          </form>
          
         
        </tr>
      <% }) %>
    </table>
